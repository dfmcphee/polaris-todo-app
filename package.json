{
  "name": "express-react",
  "version": "0.0.1",
  "description": "A simple boilerplate for using react with express.",
  "main": "server.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/dfmcphee/express-react.git"
  },
  "keywords": [
    "express",
    "webpack",
    "node",
    "react"
  ],
  "author": "Dominic McPhee",
  "license": "MIT",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --compilers js:babel-core/register --recursive --require ./test/setup.js",
